<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contraiteun"
    android:layout_width="match_parent"
    android:layout_height="280dp"
    android:background="#1B4A5B">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintchecker"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="2dp"
        android:background="#569CB3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/tauxdehydratext"
            android:layout_width="95dp"
            android:layout_height="30dp"
            android:gravity="center"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/eau"
            app:layout_constraintEnd_toEndOf="@+id/textView7"
            app:layout_constraintStart_toEndOf="@+id/eau" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:gravity="center"
            android:text="Taux de Déshydratation"
            app:layout_constraintBottom_toTopOf="@+id/eau"
            app:layout_constraintStart_toEndOf="@+id/eau" />

        <TextView
            android:id="@+id/tauxplantetext"
            android:layout_width="95dp"
            android:layout_height="30dp"
            android:gravity="center"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/eau2"
            app:layout_constraintEnd_toEndOf="@+id/textView3"
            app:layout_constraintStart_toEndOf="@+id/eau2" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/survie"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginBottom="8dp"
            android:src="@drawable/iconblacksurvie"
            app:civ_border_width="0dp"
            app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintStart_toEndOf="@+id/imageregiondesurvie"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <TextView
            android:id="@+id/texttyperegion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageregiondesurvie" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/imageregiondesurvie"
            android:layout_width="190dp"
            android:layout_height="190dp"
            android:src="@drawable/regiondesert"
            app:civ_border_color="#1B4A5B"
            app:civ_border_width="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.302" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/eau"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginBottom="8dp"
            android:src="@drawable/iconblackeau"
            app:civ_border_color="#FF000000"
            app:civ_border_width="2dp"
            app:layout_constraintBottom_toTopOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="@+id/survie"
            app:layout_constraintStart_toStartOf="@+id/survie" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/eau2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/iconblackplante"
            app:civ_border_color="#FF000000"
            app:civ_border_width="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageregiondesurvie"
            app:layout_constraintEnd_toEndOf="@+id/eau"
            app:layout_constraintStart_toStartOf="@+id/eau" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Taux de Survie"
            app:layout_constraintEnd_toEndOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/textView7"
            app:layout_constraintTop_toTopOf="@+id/imageregiondesurvie" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Taux Plante Comestible"
            app:layout_constraintBottom_toTopOf="@+id/eau2"
            app:layout_constraintEnd_toEndOf="@+id/textView7"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView7" />

        <TextView
            android:id="@+id/tauxsurvietext"
            android:layout_width="95dp"
            android:layout_height="30dp"
            android:gravity="center"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/survie"
            app:layout_constraintEnd_toEndOf="@+id/tauxdehydratext"
            app:layout_constraintStart_toStartOf="@+id/tauxdehydratext" />

        <TextView
            android:id="@+id/texttitretype"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:text="Milieu Déssertique"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>


